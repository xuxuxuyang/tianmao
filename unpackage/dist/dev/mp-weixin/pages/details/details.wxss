/* 自定义按钮 */
.video-img image{width: 90rpx; height: 90rpx; z-index: 999;
border: 2px solid #FFFFFF;
border-radius: 50%;}
.video-img{width: 90rpx; height: 90rpx;
position: absolute; bottom: 0;
left: 50%;
top: 50%;
-webkit-transform: translateX(-50%) translateY(-50%);
        transform: translateX(-50%) translateY(-50%);
}
.video-btn{height: 700rpx; position: relative;}

